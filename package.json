{
  "name": "healthcare-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "style": "prettier --check '**/src/**/*.{scss,ts}'",
    "style:fix": "prettier --write '**/src/**/*.{scss,ts}'",
    "lint": "ng lint --max-warnings=0",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && start-storybook -p 6006",
    "build-storybook": "yarn docs:json && build-storybook",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{html,scss,ts}": [
      "prettier --check"
    ],
    "src/**/*.{html,ts}": [
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@angular/animations": "~14.2.3",
    "@angular/common": "~14.2.3",
    "@angular/compiler": "~14.2.3",
    "@angular/core": "~14.2.3",
    "@angular/elements": "^14.2.3",
    "@angular/forms": "~14.2.3",
    "@angular/platform-browser": "~14.2.3",
    "@angular/platform-browser-dynamic": "~14.2.3",
    "@angular/router": "~14.2.3",
    "query-string": "^7.1.1",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.3",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "~14.2.3",
    "@babel/core": "^7.19.1",
    "@compodoc/compodoc": "^1.1.19",
    "@cypress/schematic": "^2.1.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/angular": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/angular": "^12.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.19",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-loader": "^8.2.5",
    "cypress": "10.8.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "jest-preset-angular": "^12.2.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.3",
    "ng-mocks": "^14.2.2",
    "node-request-interceptor": "^0.6.3",
    "prettier": "^2.7.1",
    "typescript": "~4.8.3",
    "uuid": "^9.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/app/**/*.ts",
      "!<rootDir>/**/*.stories.ts",
      "!<rootDir>/**/*.module.ts",
      "!<rootDir>/src/app/modules/docs/**/*.ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  },
  "msw": {
    "workerDirectory": "src"
  }
}
