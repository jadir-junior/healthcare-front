{
  "name": "healthcare-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "style": "prettier --check '**/src/**/*.{scss,ts}'",
    "style:fix": "prettier --write '**/src/**/*.{scss,ts}'",
    "lint": "ng lint --max-warnings=0",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{html,scss,ts}": [
      "prettier --check"
    ],
    "src/app/**/*.{html,ts}": [
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@angular/animations": "~16.2.9",
    "@angular/common": "~16.2.9",
    "@angular/compiler": "~16.2.9",
    "@angular/core": "~16.2.9",
    "@angular/elements": "^16.2.9",
    "@angular/forms": "~16.2.9",
    "@angular/platform-browser": "~16.2.9",
    "@angular/platform-browser-dynamic": "~16.2.9",
    "@angular/router": "~16.2.9",
    "@ng-select/ng-select": "^11.1.1",
    "@ngneat/input-mask": "^6.1.0",
    "date-fns": "^2.30.0",
    "inputmask": "^5.0.8",
    "lodash": "^4.17.21",
    "ngx-markdown": "^16.0.0",
    "prism-themes": "^1.9.0",
    "query-string": "^7.1.3",
    "rxjs": "~7.8.1",
    "tslib": "^2.4.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "~16.2.6",
    "@angular/compiler-cli": "~16.2.9",
    "@babel/core": "^7.23.2",
    "@compodoc/compodoc": "^1.1.22",
    "@cypress/schematic": "2.5.1",
    "@testing-library/angular": "14.3.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/user-event": "14.5.1",
    "@types/events": "^3.0.2",
    "@types/inputmask": "^5.0.5",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/node": "20.8.7",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "babel-loader": "^9.1.3",
    "cypress": "13.3.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "8.0.3",
    "jest-preset-angular": "^13.1.2",
    "lint-staged": "15.0.2",
    "msw": "^1.3.2",
    "ng-mocks": "^14.11.0",
    "node-request-interceptor": "^0.6.3",
    "prettier": "3.0.3",
    "typescript": "5.1.6"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/app/**/*.ts",
      "!<rootDir>/**/*.stories.ts",
      "!<rootDir>/**/*.module.ts",
      "!<rootDir>/src/app/modules/docs/**/*.ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "transform": {
      "^.+\\.(ts|js|html)?$": [
        "jest-preset-angular",
        {
          "tsconfig": "<rootDir>/tsconfig.spec.json",
          "stringifyContentPathRegex": "\\.html$"
        }
      ]
    }
  },
  "msw": {
    "workerDirectory": "src"
  }
}
